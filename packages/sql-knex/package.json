{
  "name": "@graphql-pagination/sql-knex",
  "version": "1.0.16",
  "description": "GraphQL Pagination - SQL Knex",
  "author": "Libor Krzyzanek",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "tsc --build --clean && rm -rf coverage"
  },
  "keywords": [
    "graphql",
    "pagination",
    "sql"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lkrzyzanek/graphql-pagination",
    "directory": "packages/sql-knex"
  },
  "bugs": {
    "url": "https://github.com/lkrzyzanek/graphql-pagination/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-pagination/core": "^1.4.3",
    "knex": "^2.1.0"
  },
  "devDependencies": {
    "better-sqlite3": "^8.0.0"
  }
}
