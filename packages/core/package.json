{
  "name": "@graphql-pagination/core",
  "version": "1.5.1",
  "description": "GraphQL Pagination Core module",
  "author": "Libor Krzyzanek",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "tsc --build --clean && rm -rf coverage"
  },
  "keywords": [
    "graphql",
    "pagination"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lkrzyzanek/graphql-pagination",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/lkrzyzanek/graphql-pagination/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "graphql-tag": "^2.0.0"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  }
}
