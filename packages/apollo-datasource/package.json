{
  "name": "@graphql-pagination/apollo-datasource",
  "version": "1.2.5",
  "description": "GraphQL Pagination - Apollo Datasource integration",
  "author": "Libor Krzyzanek",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "tsc --build --clean && rm -rf coverage"
  },
  "keywords": [
    "graphql",
    "pagination",
    "apollo-datasource"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lkrzyzanek/graphql-pagination",
    "directory": "packages/apollo-datasource"
  },
  "bugs": {
    "url": "https://github.com/lkrzyzanek/graphql-pagination/issues"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-pagination/core": "^1.4.5",
    "apollo-datasource": "^3.3.1",
    "apollo-server-errors": "^3.3.1"
  }
}
